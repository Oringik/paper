#!/home/oringik/paper/env/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'paper','console_scripts','paper'
__requires__ = 'paper'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('paper', 'console_scripts', 'paper')()
    )
